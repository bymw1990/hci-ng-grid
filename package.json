{
  "name": "hci-ng-grid",
  "version": "5.1.1",
  "description": "Comprehensive angular grid component",
  "keywords": [
    "angular",
    "angular2",
    "data",
    "grid",
    "list",
    "table"
  ],
  "license": "MIT",
  "repository": "git+https://github.com/HuntsmanCancerInstitute-RISR/hci-ng-grid.git",
  "homepage": "https://huntsmancancerinstitute-risr.github.io/hci-ng-grid-demo",
  "scripts": {
    "build": "npm run clean && npm run ngc && npm run doc &&  npm run copyfiles package.json dist/ && npm run copyfiles -- src/grid/themes/*.css dist/ && npm run push",
    "build-demo": "gulp build-demo",
    "clean": "npm run rimraf index.js index.d.ts index.ngsummary.json index.metadata.json index.js.map src/**/*.js src/**/*.ngfactory.ts src/**/*.d.ts src/**/*.js.map src/**/*.ngsummary.json src/**/*.metadata.json code-coverage dist",
    "compodoc": "compodoc",
    "copyfiles": "copyfiles",
    "copyToDist": "npm run copyfiles -- package.json README.md dist/",
    "doc": "npm run compodoc -- src -p ./tsconfig.json -d docs -n 'hci-ng-grid'",
    "gulp": "gulp",
    "install-demo": "gulp install-demo",
    "karma": "karma",
    "ngc": "ngc",
    "npmPack": "npm run build && npm run copyToDist && cd dist && npm pack",
    "npmPublish": "npm run build && npm run copyToDist && cd dist && npm publish --access public",
    "push": "cd dist && npm run copyfiles -- \"index.+(js|d.ts|js.map|metadata.json)\" ../demo/node_modules/hci-ng-grid && npm run copyfiles -- -e \"*.+(ngfactory|ngsummary).*\" \"src/**/*.+(js|d.ts|js.map|metadata.json|css)\" ../demo/node_modules/hci-ng-grid && cd ..",
    "rimraf": "rimraf",
    "run-demo": "gulp run-demo",
    "run-demo-prod": "gulp run-demo-prod",
    "push-hci-ng": "gulp push-libs-dest --dest ../hci-ng"
  },
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.+(js|d.ts|js.map|metadata.json)",
    "src/**/!(*spec).+(js|d.ts|js.map|metadata.json|css)"
  ],
  "dependencies": {
    "hci-ng-grid-dto": "1.0.x",
    "moment": ">=2.19.3",
    "rxjs-compat": "^6.5.3"
  },
  "devDependencies": {
    "@angular/animations": ">=8.0.0 <10.0.0",
    "@angular/cdk": ">=8.0.0 <10.0.0",
    "@angular/common": ">=8.0.0 <10.0.0",
    "@angular/compiler": ">=8.0.0 <10.0.0",
    "@angular/compiler-cli": ">=8.0.0 <10.0.0",
    "@angular/core": ">=8.0.0 <10.0.0",
    "@angular/forms": ">=8.0.0 <10.0.0",
    "@angular/material": ">=8.0.0 <10.0.0",
    "@angular/platform-browser": ">=8.0.0 <10.0.0",
    "@angular/platform-browser-dynamic": ">=8.0.0 <10.0.0",
    "@angular/router": ">=8.0.0 <10.0.0",
    "@compodoc/compodoc": "1.1.x",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.0",
    "@types/webpack": "4.1.2",
    "angular2-cool-storage": "3.1.5",
    "awesome-typescript-loader": "^3.2.3",
    "angular2-template-loader": "^0.6.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "cryptiles": ">=4.1.2",
    "css-loader": "^0.26.0",
    "del": "^2.2.1",
    "deep-extend": ">=0.5.1",
    "event-stream": "3.3.4",
    "extend": ">=3.0.2",
    "fs": "^0.0.2",
    "fs-extra": "7.0.x",
    "gulp": "4.0.x",
    "gulp-clean": "0.4.x",
    "gulp-debug": "4.0.x",
    "gulp-exec": "3.0.x",
    "gulp-exit": "0.0.x",
    "gulp-install": "1.1.x",
    "gulp-json-editor": "2.4.x",
    "gulp-json-modify": "1.0.x",
    "gulp-load-plugins": "1.5.x",
    "gulp-rename": "1.4.x",
    "gulp-sequence": "1.0.x",
    "immutable": "^3.8.1",
    "inline-source-map": "^0.6.2",
    "istanbul-instrumenter-loader": "3.0.x",
    "jasmine-core": "^2.6.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lodash": ">=4.17.11",
    "macaddress": ">=0.2.9",
    "minimist": "^1.2.0",
    "node-color-log": "2.1.x",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.6.1",
    "rxjs": "6.4.x",
    "semver": "5.6.x",
    "source-map-loader": "^0.2.1",
    "tar": ">=4.4.2",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.5.1",
    "typescript": "~3.4.2",
    "url-parse": ">=1.4.3",
    "webpack": "3.11.x",
    "webpack-dev-middleware": "1.12.x",
    "webpack-dev-server": "2.7.x",
    "webpack-merge": "4.1.x",
    "zone.js": "^0.9.1"
  }
}
